{
    "version": 4,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "use-microLIB": true,
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-3",
        "one-elf-section-per-function": true,
        "c99-mode": true,
        "C_FLAGS": "--reduce_paths",
        "CXX_FLAGS": "--reduce_paths",
        "warnings": "unspecified"
    },
    "asm-compiler": {
        "misc-controls": "--cpreproc_opts=-DBOARD_PCA10040,-DCONFIG_GPIO_AS_PINRESET,-DFLOAT_ABI_HARD,-DFREERTOS,-DNRF52,-DNRF52832_XXAA,-DNRF52_PAN_74,-DNRF_SD_BLE_API_VERSION=7,-DS132,-DSOFTDEVICE_PRESENT,-D__HEAP_SIZE=1024,-D__STACK_SIZE=2048"
    },
    "linker": {
        "output-format": "elf",
        "xo-base": "",
        "ro-base": "0x00000000",
        "rw-base": "0x20000000",
        "misc-controls": "--diag_suppress 6330"
    }
}
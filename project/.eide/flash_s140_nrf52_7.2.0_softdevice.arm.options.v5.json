{
    "version": 4,
    "beforeBuildTasks": [
        {
            "name": "attrib +R $H\\*",
            "command": "cd .\\..\\nrf_sdk\\examples\\ble_peripheral\\ble_app_hrs_freertos\\pca10056\\s140\\arm5_no_packs && attrib +R ${OutDir}\\\\*",
            "disable": false,
            "abortAfterFailed": true,
            "stopBuildAfterFailed": true
        }
    ],
    "afterBuildTasks": [],
    "global": {
        "use-microLIB": true,
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-3",
        "one-elf-section-per-function": true,
        "c99-mode": true,
        "C_FLAGS": "--reduce_paths",
        "CXX_FLAGS": "--reduce_paths",
        "warnings": "unspecified"
    },
    "asm-compiler": {
        "misc-controls": "--cpreproc_opts=-D__HEAP_SIZE=1024,-D__STACK_SIZE=2048"
    },
    "linker": {
        "output-format": "elf",
        "xo-base": "",
        "ro-base": "0x00000000",
        "rw-base": "0x20000000",
        "misc-controls": "--diag_suppress 6330"
    }
}